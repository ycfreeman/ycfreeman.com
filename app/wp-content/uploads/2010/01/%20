<script type="text/javascript">

jQuery(window).load(function(){
  if (jQuery.browser.safari && document.readyState != "complete"){
    //console.info('ready...');
    setTimeout( arguments.callee, 100 );
    return;
  } 
  var newsletter_box = jQuery("#newsletter-box");
  var newsletter_offset = 0;
  if (newsletter_box) {
    newsletter_offset = newsletter_box.outerHeight();
  }
  
  var countdown_offset = 0;
  
  var offsets = new Array();
  offsets["box-center"] = countdown_offset;
  offsets["box-left"] = 0;
  offsets["box-right"] = newsletter_offset + 8;
  
  jQuery('#box-center, #box-left, #box-right').equalizeCols( offsets );
});

</script>

