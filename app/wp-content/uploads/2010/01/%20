<script type='text/javascript' charset='utf-8'>
var overlay_wrapper;
var overlay_panel;
function append_overlay() {
overlay_wrapper = jQuery('<div id="modaltrailer"></div>').appendTo( jQuery('BODY') );
overlay_panel_content = "<div id='trailerbox'><div id='trailerbox-inner'><img src='http://www.torchlightgame.com/assets/images/trailer-title-media-ie.png' border='0' class='trailerbox-title' /><div id='trailer-content-box'><div style='margin-bottom:33px;'><a href='http://www.adobe.com/go/EN_US-H-GET-FLASH' target='_blank'>Please install the latest version of Flash to view our trailer.</a></div></div><div id='trailer-textbox' style='margin: 0px; padding: 0px;'></div><a id='trailer-close-button' class='trailer-content-box-button' href='javascript:close_overlay();'> </a></div></div>";
overlay_panel = jQuery(overlay_panel_content).appendTo(overlay_wrapper); 
};
function show_overlay(panel_function) {
if (!overlay_wrapper) append_overlay();
setTimeout(panel_function + '()', 500);
overlay_wrapper.fadeIn(700);
}
function clear_flashplayer(element_id) {
swfobject.embedSWF('http://www.torchlightgame.com/assets/flash/empty.swf', element_id, '1', '1', '10.0.0', null, null, {wmode:'transparent'}, null);
}
function close_overlay() {
setTimeout("clear_flashplayer('trailer-content-box');", 500);
jQuery('#trailer-textbox').empty();
hide_overlay();
}
function hide_overlay() {
setTimeout('overlay_wrapper.fadeOut(500);', 500);
raise_panel();
}
function raise_panel() {
setTimeout("jQuery('#trailerbox-inner').hide();", 800)
overlay_panel.animate({ marginTop: "+=10px"}, 100).animate({ marginTop: "-600px"}, 600);
}
</script>
<script type="text/javascript">
var destroyertrailer = function() {
  $overlay_panel = jQuery("#trailerbox");
  if (jQuery.browser.msie) {
    $overlay_panel.animate({ marginTop: "-135px"}, 600).animate({ marginTop: "-=40px"}, 140).animate({ marginTop: "+=40px"}, 140).animate({ marginTop: "-=10px"}, 140).animate({ marginTop: "+=10px"}, 140);
  } else {
    $overlay_panel.animate({ marginTop: "-300px"}, 600).animate({ marginTop: "-=40px"}, 140).animate({ marginTop: "+=40px"}, 140).animate({ marginTop: "-=10px"}, 140).animate({ marginTop: "+=10px"}, 140);
  }
  
  $textbox_text = "";
  $is_link = true;
  if ("http://www.youtube.com/watch?v=NhrNZZf-dOk" != "") $is_link = true;
  
  if ($is_link) $textbox_text = "<a href='http://www.youtube.com/watch?v=NhrNZZf-dOk' target='_blank'>";
  $textbox_text += "Watch this video on Youtube";
  if ($is_link) $textbox_text += "</a>";
  
  jQuery("#trailer-textbox").append($textbox_text);
  
  jQuery("#trailerbox-inner").show();
  
  var flashvars = {};
	var params = {};
	params.wmode = "transparent";
	var attributes = {};
	attributes.id = "trailer-content-box";
	swfobject.embedSWF("/assets/flash/destroyer-trailer.swf", "trailer-content-box", "500", "320", "10.0.0", "/assets/flash/expressInstall.swf", flashvars, params, attributes);
}
